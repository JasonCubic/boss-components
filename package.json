{
  "name": "bs-components",
  "version": "0.0.4",
  "description": "Bootstrap Stenciljs Components",
  "author": "Jason Cubic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JasonCubic/bs-components"
  },
  "homepage": "https://github.com/JasonCubic/bs-components",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JasonCubic/bs-components.git"
  },
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "build-es5": "stencil build --es5",
    "start": "stencil build --dev --watch --serve",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "test-bs-tooltip": "stencil build && testcafe ie,chrome ./tests/bs-tooltip.test.ts",
    "test-bs-button": "stencil build && testcafe ie,chrome ./tests/bs-button.test.ts",
    "test-bs-modal": "stencil build && testcafe ie,chrome ./tests/bs-modal.test.ts",
    "test-bs-popover": "stencil build && testcafe ie,chrome ./tests/bs-popover.test.ts",
    "test-bs-dropdown": "stencil build && testcafe ie,chrome ./tests/bs-dropdown.test.ts",
    "test-bs-collapse": "stencil build && testcafe ie,chrome ./tests/bs-collapse.test.ts",
    "test-bs-alert": "stencil build && testcafe chrome ./tests/bs-alert.test.ts"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "popper.js": "^1.14.4"
  },
  "devDependencies": {
    "@stencil/core": "^0.11.4",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-typescript": "^0.12.0",
    "testcafe": "^0.21.1",
    "typescript-eslint-parser": "^18.0.0",
    "vuepress": "^0.14.2"
  }
