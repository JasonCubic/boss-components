{
  "name": "bs-components",
  "version": "0.0.2",
  "description": "Bootstrap Stenciljs Components",
  "author": "Jason Cubic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JasonCubic/bs-components"
  },
  "homepage": "https://github.com/JasonCubic/bs-components",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JasonCubic/bs-components.git"
  },
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "build-es5": "stencil build --es5",
    "start": "stencil build --dev --watch --serve",
    "start-es5": "stencil build --dev --watch --serve --es5",
    "test": "jest",
    "test.watch": "jest --watch",
    "test-bs-tooltip": "stencil build && testcafe ie,chrome ./tests/bs-tooltip.test.ts",
    "test-bs-button": "stencil build && testcafe ie,chrome ./tests/bs-button.test.ts",
    "test-bs-modal": "stencil build && testcafe chrome ./tests/bs-modal.test.ts --hostname CTNAL0042043380.us164.corpintra.net --proxy webproxy.us164.corpintra.net:8080"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "popper.js": "^1.14.4"
  },
  "devDependencies": {
    "@stencil/core": "^0.11.4",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.116",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-typescript": "^0.12.0",
    "jest": "^23.4.2",
    "testcafe": "^0.21.1",
    "typescript-eslint-parser": "^18.0.0"
  },
  "jest": {
    "testURL": "http://localhost/",
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
