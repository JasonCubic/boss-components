{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.options": {
    "extensions": [
      ".js",
      ".ts",
      ".tsx"
    ]
  },
  "cSpell.words": [
    "describedby",
    "dropdown",
    "keycode",
    "linebreak",
    "nowrap",
    "scrollbar",
    "tabindex",
    "testcafe",
    "tooltips",
    "untoggle"
  ]
}
